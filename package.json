  },
  "engines": {
    "node": ">=8"
  },
  "author": "2017+ Kreig Zimmerman <kreig303@yahoo.com> (http://kreig.me)",
  "maintainers": [
    "Kreig Zimmerman <kreig303@yahoo.com> (http://kreig.me)"
  ],
  "contributors": [
    "Kreig Zimmerman <kreig303@yahoo.com> (http://kreig.me)",
    "Greenkeeper (http://greenkeeper.io/)"
  ],
  "license": "MIT",
  "badges": {
    "list": [
      "travisci",
      "daviddm",
      "daviddmdev"
    ],
    "config": {}
  },
  "dependencies": {
    "boom": "^6.0.0",
    "confidence": "^3.0.2",
    "dotenv": "^4.0.0",
