    "lout": "^10.0.2",
    "request": "^2.83.0",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "code": "^5.1.0",
    "eslint": "^4.8.0",
    "eslint-config-hapi": "^10.1.0",
    "eslint-plugin-security": "^1.4.0",
